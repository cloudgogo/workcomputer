<TableData name="MR_权益风险-长期股权投资明细" class="com.fr.data.impl.DBTableData">
<Parameters>
<Parameter>
<Attributes name="p_period"/>
<O>
<![CDATA[2018-02]]></O>
</Parameter>
</Parameters>
<Attributes maxMemRowCount="-1"/>
<Connection class="com.fr.data.impl.NameDatabaseConnection">
<DatabaseName>
<![CDATA[HRS]]></DatabaseName>
</Connection>
<Query>
<![CDATA[select c.cjfl,
       v1.description desc1,
       c.tzxs,
       v2.description desc2,
       c.segment8,
       c.description,
       c.maturity_date yqrq,
       SUM(c.book_value) book_value,
       to_char(max(c.last_update_date), 'yyyy-mm-dd') last_update_date
  from hrs_core_economic_capital c,
  (select v.flex_value,v.description
   from
    hrs_def_flex_value_sets s,
    hrs_def_flex_values v
    where s.flex_value_set_id=v.flex_value_set_id
    and s.flex_value_set_name='JJZB_BC_PROD_SUB_CATE') v1,
    (select v.flex_value,v.description
   from
    hrs_def_flex_value_sets s,
    hrs_def_flex_values v
    where s.flex_value_set_id=v.flex_value_set_id
    and s.flex_value_set_name='JJZB-BC-长期股权投资对象') v2
  where 1=1
   and c.period_name = '${p_period}'
  and c.attribute5='1110'
  and c.cjfl=v1.flex_value
  and c.tzxs=v2.flex_value
  and c.segment1 not in ('10401','10402','10403','10404','10405A','105')
 group by c.cjfl,
