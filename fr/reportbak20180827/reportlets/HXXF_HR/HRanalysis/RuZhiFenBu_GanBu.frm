<TableData name="各月度" class="com.fr.data.impl.DBTableData">
<Parameters>
<Parameter>
<Attributes name="seadate"/>
<O>
<![CDATA[]]></O>
</Parameter>
<Parameter>
<Attributes name="DWMC"/>
<O>
<![CDATA[HX_HEAD]]></O>
</Parameter>
<Parameter>
<Attributes name="xilie"/>
<O>
<![CDATA[]]></O>
</Parameter>
</Parameters>
<Attributes maxMemRowCount="-1"/>
<Connection class="com.fr.data.impl.NameDatabaseConnection">
<DatabaseName>
<![CDATA[HXXF_HR]]></DatabaseName>
</Connection>
<Query>
<![CDATA[WITH 
A AS
(
SELECT COUNT(YGID) TOTNUM
  FROM ODM_HR_RZTZ
 WHERE 1=1
 ${if(len(seadate)==0 ,
"AND TO_CHAR(RZDATE,'YYYY') = TO_CHAR(SYSDATE,'YYYY')",
"AND TO_CHAR(RZDATE,'YYYY') = '"+seadate+"'")}
    AND DQYGFENLEI = '干部'
    ${if(len(DWMC)=0,""," and TREE_NODE_ID LIKE '%"+DWMC+",%'")}
)

SELECT T.FENLEI,T.RENSHU,T.RENSHU / A.TOTNUM AS ZHANBI,T.RENSHU || '.0' || round((T.RENSHU / A.TOTNUM)*1000,0) TEMP
  FROM(
SELECT EXTRACT(MONTH FROM RZDATE)  AS FENLEI,
       COUNT(YGID) RENSHU
  FROM ODM_HR_RZTZ
 WHERE 1=1
   AND DQYGFENLEI = '干部'
 ${if(len(seadate)==0 ,
"AND TO_CHAR(RZDATE,'YYYY') = TO_CHAR(SYSDATE,'YYYY')",
"AND TO_CHAR(RZDATE,'YYYY') = '"+seadate+"'")}

${if(len(DWMC)=0,""," and TREE_NODE_ID LIKE '%"+DWMC+",%'")}
  ${if(len(xilie) == 0 || xilie == 'renshu',"",if(xilie == '在职',"AND ISLIZHI = '在职'","AND ISLIZHI = '离职'"))}
 GROUP BY EXTRACT(MONTH FROM RZDATE)
 ORDER BY FENLEI) T,A]]></Query>
</TableData>
<TableData name="各单位" class="com.fr.data.impl.DBTableData">
<Parameters>
<Parameter>
<Attributes name="seadate"/>
<O>
<![CDATA[]]></O>
</Parameter>
<Parameter>
<Attributes name="DWMC"/>
<O>
<![CDATA[]]></O>
</Parameter>
<Parameter>
<Attributes name="xilie"/>
<O>
<![CDATA[]]></O>
</Parameter>
</Parameters>
<Attributes maxMemRowCount="-1"/>
<Connection class="com.fr.data.impl.NameDatabaseConnection">
<DatabaseName>
<![CDATA[HXXF_HR]]></DatabaseName>
</Connection>
<Query>
<![CDATA[WITH
A AS(
select count(ygid) RENSHU,
       descr
  from (select t1.ygid, 
               t2.descr
          from odm_hr_rztz t1, odm_hr_dw t2
         where 
           ${if(len(xilie) == 0 || xilie == 'renshu',"",if(xilie == '在职',"ISLIZHI = '在职' AND ","ISLIZHI = '离职' AND "))}
         t1.DQYGFENLEI='干部' and 
         t1.tree_node_id like '%' || t2.tree_node || '%' 
                   ${if(len(DWMC)=0," AND   PARENT_NODE ='HX_HEAD' ","AND PARENT_NODE ='"+DWMC+"'" )}--参数 
${if(len(seadate)==0 ,
"AND TO_CHAR(RZDATE,'YYYY') = TO_CHAR(SYSDATE,'YYYY')",
"AND TO_CHAR(RZDATE,'YYYY') = '"+seadate+"'")}

union

select t1.ygid, 
               t2.descr
          from odm_hr_rztz t1, odm_hr_dw t2
         where 
           ${if(len(xilie) == 0 || xilie == 'renshu',"",if(xilie == '在职',"ISLIZHI = '在职' AND ","ISLIZHI = '离职' AND "))}
         t1.DQYGFENLEI='干部' and 
         t1.tree_node_id like '%' || t2.tree_node || '%' 
        ${if(len(DWMC)=0," AND   PARENT_NODE ='HX_HEAD' ","AND BUMENID ='"+DWMC+"'" )}--参数 
         and bumenid = t2.tree_node 
${if(len(seadate)==0 ,
"AND TO_CHAR(RZDATE,'YYYY') = TO_CHAR(SYSDATE,'YYYY')",
"AND TO_CHAR(RZDATE,'YYYY') = '"+seadate+"'")}
                                      )
                  group by descr
 ORDER BY RENSHU DESC
 ),

 B AS(
SELECT SUM(RENSHU) AS RENSHU
  FROM A
  ),

C AS(
SELECT COUNT(YGID) RENSHU
  FROM ODM_HR_RZTZ
 WHERE 1=1
${if(len(seadate)==0 ,
"AND TO_CHAR(RZDATE,'YYYY') = TO_CHAR(SYSDATE,'YYYY')",
"AND TO_CHAR(RZDATE,'YYYY') = '"+seadate+"'")}
   AND DQYGFENLEI = '干部'
   ${if(len(xilie) == 0 || xilie == 'renshu',"",if(xilie == '在职',"AND ISLIZHI = '在职'","AND ISLIZHI = '离职'"))}
${if(len(DWMC)=0,"and 1=1"," and TREE_NODE_ID LIKE '%"+DWMC+",%'")}
)


select *
from
(

SELECT *
  FROM A

  UNION ALL 
  
  SELECT C.RENSHU - B.RENSHU AS RENSHU,
      '其他（已撤单位）' as descr
  FROM B,C
  )
  where renshu > 0

 ]]></Query>
</TableData>
