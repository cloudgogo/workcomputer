${if(len(temp)==0,
 "  UNION ALL 
  SELECT C.RENSHU - B.RENSHU AS RENSHU,
      '其他（已撤单位）' as descr
  FROM B,C",
  "  UNION ALL 
  SELECT "+temp+" - B.RENSHU AS RENSHU,
      '其他（已撤单位）' as descr
