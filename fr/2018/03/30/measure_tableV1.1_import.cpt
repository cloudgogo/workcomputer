<TableData name="MR_房地产价格风险" class="com.fr.data.impl.DBTableData">
<Parameters>
<Parameter>
<Attributes name="p_period"/>
<O>
<![CDATA[2018-02]]></O>
</Parameter>
</Parameters>
<Attributes maxMemRowCount="-1"/>
<Connection class="com.fr.data.impl.NameDatabaseConnection">
<DatabaseName>
<![CDATA[HRS]]></DatabaseName>
</Connection>
<Query>
<![CDATA[select t.*,
      case when t.fdcbd>=1 
      then 1 when t.fdcbd>0
      then t.fdcbd*t.fdcbd
      else
      (-1)*(t.fdcbd*t.fdcbd)
      end k1
      from (
select c.segment8,
       c.Description,
       c.attribute5,
       C.PROD_TYPE2,
       c.attribute4,
       c.cyxs,
       cyxs.description desc1,
       c.jjfs,
       jjfs.description desc2,
       scwz.description scwz,
       case when SUM(c.book_cost)=0
       then 0
       else round((SUM(c.book_value)-SUM(c.book_cost))/SUM(c.book_cost),0)
       end fdcbd,
       SUM(c.book_cost) Book_Cost,
       SUM(c.book_value) book_value,
       case when  jjfs.description='以历史成本计价'
       then SUM(c.book_value)*0.08
       when jjfs.description='以公允价值计价' then
       SUM(c.book_value)*0.12 
       else
       SUM(c.book_value)*0.08
       end book_value2,
       to_char(max(c.last_update_date),'yyyy-mm-dd') last_update_date
  from hrs_core_economic_capital c,
       (select v.flex_value,v.description from 
       hrs_def_flex_value_sets s,
       hrs_def_flex_values v
       where s.flex_value_set_name='JJZB-房地产-持有形式'
       and s.flex_value_set_id=v.flex_value_set_id) cyxs,
       (select v.flex_value,v.description from 
       hrs_def_flex_value_sets s,
       hrs_def_flex_values v
       where s.flex_value_set_name='JJZB-房地产-计价方式'
       and s.flex_value_set_id=v.flex_value_set_id) jjfs,
       (select v.flex_value,v.description from 
       hrs_def_flex_value_sets s,
       hrs_def_flex_values v
       where s.flex_value_set_name='JJZB-房地产-所处位置'
       and s.flex_value_set_id=v.flex_value_set_id) scwz,
       hrs_core_financial_product p
 where c.attribute5=p.detail_category

   and c.period_name = '${p_period}'
   and cyxs.flex_value(+)=c.cyxs
   and jjfs.flex_value(+)=c.jjfs
   and scwz.flex_value(+)=c.scwz
   and p.asset_category='410'
 group by c.segment8,
       c.Description,
       c.attribute5,
       C.PROD_TYPE2,
       c.attribute4,
        c.cyxs,
       cyxs.description,
       c.jjfs,
       jjfs.description,
       c.scwz,
       scwz.description
  order by  c.cyxs,
         c.jjfs,
       c.segment8) t]]></Query>
</TableData>
