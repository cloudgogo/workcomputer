<Query>
<![CDATA[select case
         when t.period_name = substr('${p_period}', 1, 4) - 1 || '-' || '13' then
          substr('${p_period}', 1, 4) - 1
         when t.period_name = substr('${p_period}', 1, 4) - 2 || '-' || '13' then
          substr('${p_period}', 1, 4) - 2
         when t.period_name = substr('${p_period}', 1, 4) - 3 || '-' || '13' then
          substr('${p_period}', 1, 4) - 3
       end period_year, 
       case
         when t.period_name = substr('${p_period}', 1, 4) - 1 || '-' || '13' then
          '01'
         when t.period_name = substr('${p_period}', 1, 4) - 2 || '-' || '13' then
