<![CDATA[1000]]></O>
</Parameter>
<Parameter>
<Attributes name="p_period_e"/>
<O>
<![CDATA[2017-12]]></O>
</Parameter>
<Parameter>
<Attributes name="p_department"/>
<O>
<![CDATA[131405\', \'131406\', \'131127\', \'131107\', \'131128]]></O>
</Parameter>
<Parameter>
<Attributes name="p_period_s"/>
<O>
<![CDATA[2017-11]]></O>
</Parameter>
<Parameter>
<Attributes name="p_company"/>
<O>
<![CDATA[]]></O>
</Parameter>
<Parameter>
<Attributes name="p_project"/>
<O>
<![CDATA[]]></O>
</Parameter>
<Parameter>
<Attributes name="p_datasource"/>
<O>
<![CDATA[MA\',\'GL\',\'XL]]></O>
</Parameter>
</Parameters>
<Attributes maxMemRowCount="-1"/>
<Connection class="com.fr.data.impl.NameDatabaseConnection">
<DatabaseName>
<![CDATA[HRS2]]></DatabaseName>
</Connection>
<Query>
<![CDATA[select result.segment2,
       result.segment6,
       -sum(result.income) income,
       sum(result.payment) payment,
       sum(result.expense) expense,
       nvl(-sum(result.income),0)-nvl(sum(result.payment),0)- nvl(sum(result.expense),0) margin
  from (select a.segment2,
               a.segment6,
               case
                 when a.segment3 between '6021' and '6111ZZ' then
                  a.amount
               end income,
               case
                 when a.segment3 between '6421' and '6421ZZ' then
                  a.amount
               end payment,
               case
                 when a.segment3 between '6601' and '6901ZZ' then
                  a.amount
               end expense
          from (
                
                select i.segment2,
                        i.segment6,
                        i.segment3,
                        nvl(i.entered_dr, 0) - nvl(i.entered_cr, 0) amount
                
                  from CUX_GL_INTERFACE_INVESTBANK i
                 where 1 = 1
                   and i.period_name between '${p_period_s}' and
                       '${p_period_e}'
                   and ${IF(LEN(p_company) == 0,
                            "1=1",
                            " i.segment1 in ('" + p_company + "')")
                 }
                   and ${IF(LEN(p_department) == 0,
                            "1=1",
                            " i.segment2 in ('" + p_department + "')")
                 }
                   and ${IF(LEN(p_project) == 0,
                            "1=1",
                            " i.segment6 in ('" + p_project + "')")
                 }
                   and ${IF(LEN(p_datasource) == 0,
                            "1=1",
                            " i.data_source in ('" + p_datasource + "')") 
                 }
                   and ${IF(LEN(p_fin_element) == 0,
                            "1=1",
                            " i.fin_element in ('" + p_fin_element + "')") }
                --  order by i.period_name, i.je_header_id, i.je_line_num
                ) a) result
 group by result.segment2, result.segment6
 order by result.segment2, result.segment6
]]></Query>
</TableData>
<TableData name="项目" class="com.fr.data.impl.DBTableData">
<Parameters/>
<Attributes maxMemRowCount="-1"/>
<Connection class="com.fr.data.impl.NameDatabaseConnection">
<DatabaseName>
<![CDATA[HRS]]></DatabaseName>
</Connection>
<Query>
<![CDATA[       SELECT DV.DIM_VALUE||'-'|| DV.DESCRIPTION DESCRIPTION,DV.DIM_VALUE
          FROM HRS_CORE_DIMENSION D, HRS_CORE_DIMENSION_VALUE DV
         WHERE D.DIMENSION_CODE = 'XXT_FIN_COA_PROJECT'
           AND DV.DIMENSION_ID = D.DIMENSION_ID
           and 
           (DV.DIM_VALUE ='0' or DV.DIM_VALUE like '131C%' or DV.DIM_VALUE like '131F%')
           ]]></Query>
</TableData>
<TableData name="部门" class="com.fr.data.impl.DBTableData">
<Parameters/>
<Attributes maxMemRowCount="-1"/>
<Connection class="com.fr.data.impl.NameDatabaseConnection">
<DatabaseName>
<![CDATA[HRS]]></DatabaseName>
</Connection>
<Query>
<![CDATA[         select distinct
                r.dept_code,
                r.dept_code||'-'||
                (select description
                   from hrs_core_dimension d, hrs_core_dimension_value dv
                  where d.dimension_id = dv.dimension_id
                    and d.dimension_code = 'XXT_FIN_COA_COSTCENTER'
                    and Dv.Dim_Value = r.dept_code) description
         
           from hrs_def_relationship_set rs, hrs_def_relationship r
          where rs.relationship_set_name =
                'HRS_LEDGER_COSTCENTER_RELATIONSHIP'
                and r.ledger_id =1001
                and  r.dept_code is not null
                  AND EXISTS (SELECT 1
          FROM  hrs_core_dim_value_hierarchy h
         WHERE 1 = 1
           AND h.dim_value = 'C131107'
           AND  r.dept_code BETWEEN h.child_dim_value_low AND
       h.child_dim_value_high)
            order by r.dept_code
                ]]></Query>
</TableData>
<TableData name="公司" class="com.fr.data.impl.DBTableData">
<Parameters/>
<Attributes maxMemRowCount="-1"/>
<Connection class="com.fr.data.impl.NameDatabaseConnection">
<DatabaseName>
<![CDATA[HRS]]></DatabaseName>
</Connection>
<Query>
<![CDATA[
--账套公司的关系  (取公司)
select  distinct
       r.company_code,
        r.company_code||'-'||(select description
          from hrs_core_dimension d, hrs_core_dimension_value dv
         where d.dimension_id = dv.dimension_id
           and d.dimension_code = 'XXT_FIN_COA_COMPANY'
           and Dv.Dim_Value = r.company_code) description

  from hrs_def_relationship_set rs, hrs_def_relationship r
 where rs.relationship_set_name = 'HRS_LEDGER_COMPANY_RELATIONSHIP'
 and r.ledger_id=1001
 and  r.company_code not like '104%'
  and  r.company_code not like '1303A'
 order by r.company_code
 ]]></Query>
</TableData>
