<Connection class="com.fr.data.impl.NameDatabaseConnection">
<DatabaseName>
<![CDATA[HXXF_HR]]></DatabaseName>
</Connection>
<Query>
<![CDATA[SELECT ROWNUM,T.*
FROM(
SELECT *
FROM ODM_HR_LZTZ
WHERE 1=1
AND ISCQT='是'
AND ISzhuying = '主营'
AND TO_CHAR(LZDATE,'YYYYMMDD')<TO_CHAR(SYSDATE,'YYYYMMDD')
${if(len(seadate)==0 ,
"AND TO_CHAR(LZDATE,'YYYY') = TO_CHAR(SYSDATE,'YYYY')",
"AND TO_CHAR(LZDATE,'YYYY') = '"+seadate+"'")}
${if(len(LIZHILEIXING)=0,"","AND LZLEIXING='"+LIZHILEIXING+"'")}
${if(len(LZYUANYIN)=0,"",if(LZYUANYIN == "其它","AND LZYUANYIN IS NULL","AND LZYUANYIN='"+LZYUANYIN+"'"))}
${SWITCH(ISSHIYONGQIN,'',"",'是',"AND ISSYQLZ='是'",'否',"AND ISSYQLZ='否'")}
${if(len(CQTCDLYEAR)=0,"","AND CQTCDLYEAR=substr('"+CQTCDLYEAR+"',0,4)")}
   ${if(len(seamonth)==0 || seamonth == 'year',
"",
"AND TO_CHAR(LZDATE,'mm') = TO_CHAR(SYSDATE,'mm')")}
${if(len(BUMEN)=0,""," and BUMENID in(
SELECT TREE_NODE
FROM ODM_HR_DW
  start with TREE_NODE IN(
SELECT TREE_NODE
FROM ODM_HR_DW
WHERE
 DESCR ='"+BUMEN+"') connect by  PARENT_NODE   =  prior TREE_NODE)")}
${if(len(DWMC)=0,"and 1=1"," and TREE_NODE_ID LIKE '%"+DWMC+",%'")}
order by LZDATE desc,ZHIJI desc
)T
]]></Query>
</TableData>
</TableDataMap>
