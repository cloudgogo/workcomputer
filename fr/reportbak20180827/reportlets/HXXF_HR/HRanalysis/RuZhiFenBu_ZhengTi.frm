<TableData name="各月度入职人数" class="com.fr.data.impl.DBTableData">
<Parameters>
<Parameter>
<Attributes name="fenlei"/>
<O>
<![CDATA[]]></O>
</Parameter>
<Parameter>
<Attributes name="seadate"/>
<O>
<![CDATA[]]></O>
</Parameter>
<Parameter>
<Attributes name="DWMC"/>
<O>
<![CDATA[]]></O>
</Parameter>
</Parameters>
<Attributes maxMemRowCount="-1"/>
<Connection class="com.fr.data.impl.NameDatabaseConnection">
<DatabaseName>
<![CDATA[HXXF_HR]]></DatabaseName>
</Connection>
<Query>
<![CDATA[WITH 
A AS
(
SELECT EXTRACT(MONTH FROM ADD_MONTHS(TO_DATE('2018-01-01','YYYY-MM-DD'),ROWNUM-1)) DAY_ID
FROM DUAL
CONNECT BY ROWNUM<=12
),

B AS
(
SELECT 
     EXTRACT(MONTH FROM RZDATE) AS FENLEI,
     COUNT(YGID) AS RENSHU
  FROM ODM_HR_RZTZ YN
 WHERE 1=1
${if(len(seadate)==0 ,
"AND TO_CHAR(RZDATE,'YYYY') = TO_CHAR(SYSDATE,'YYYY')",
"AND TO_CHAR(RZDATE,'YYYY') = '"+seadate+"'")}
${IF(fenlei == '总体' || LEN(fenlei) == 0,
   "",
   IF(fenlei == '总体-在职',
      "AND ISLIZHI = '在职'",
      IF(fenlei == '总体-离职',
         "AND ISLIZHI = '离职'",
         IF(fenlei == '主营-总体',
            "AND ISZHUYING = '主营'",
            IF(fenlei == '主营-在职',
               "AND ISZHUYING = '主营' AND ISLIZHI = '在职'",
               IF(fenlei == '主营-离职',
                  "AND ISZHUYING = '主营' AND ISLIZHI = '离职'",
                  IF(fenlei == '非主营-总体',
                     "AND ISZHUYING <>'主营'",
                     IF(fenlei == '非主营-在职',
                        "AND ISZHUYING <>'主营' AND ISLIZHI = '在职'",
                        "AND ISZHUYING <>'主营' AND ISLIZHI = '离职'"
                      )
                    )
                ) 
            )
          )
        )
    )
  )}
${if(len(DWMC)=0,"and 1=1"," and TREE_NODE_ID LIKE '%"+DWMC+",%'")}
 GROUP BY EXTRACT(MONTH FROM RZDATE)
)


SELECT 
       A.DAY_ID || '月' AS FENLEI,
       B.RENSHU
  FROM A
  LEFT JOIN B
    ON A.DAY_ID = B.FENLEI
 ORDER BY DAY_ID
]]></Query>
</TableData>
<TableData name="各部门" class="com.fr.data.impl.DBTableData">
<Parameters>
<Parameter>
<Attributes name="fenlei"/>
<O>
<![CDATA[]]></O>
</Parameter>
<Parameter>
<Attributes name="DWMC"/>
<O>
<![CDATA[]]></O>
</Parameter>
</Parameters>
<Attributes maxMemRowCount="-1"/>
<Connection class="com.fr.data.impl.NameDatabaseConnection">
<DatabaseName>
<![CDATA[HXXF_HR]]></DatabaseName>
</Connection>
<Query>
<![CDATA[SELECT  
      COUNT(YGID) AS renshu,
      --ISZHUYING,
      DANWEI
FROM (
      SELECT 
      YGID,
      T1.DESCR AS DANWEI,
      --(CASE WHEN ISZHUYING like '主营%' THEN '主营' ELSE '非主营' END) AS ISZHUYING,
      BUMENID
      TREE_NODE,
      PARENT_NODE
      FROM ODM_HR_RZTZ T ,(SELECT T4.TREE_NODE,T4.PARENT_NODE,T3.DESCR   
                              FROM (SELECT TREE_NODE,connect_by_root PARENT_NODE AS PARENT_NODE
                        FROM ODM_HR_DW 
                        start with PARENT_NODE IN (SELECT TREE_NODE  
                               FROM odm_hr_dw_old 
                                WHERE 1=1
 ${if(len(DWMC)=0," AND   PARENT_NODE ='HX_HEAD' ","AND PARENT_NODE ='"+DWMC+"'" )}
) 
                            connect by  prior TREE_NODE=  PARENT_NODE  
                            ) T4, odm_hr_dw_old T3
WHERE T3.TREE_NODE=T4.PARENT_NODE) T1
WHERE 1=1
 AND TREE_NODE=BUMENID
 AND TO_CHAR(RZDATE,'YYYY') = TO_CHAR(SYSDATE,'YYYY')
${IF(fenlei == '总体' || LEN(fenlei) == 0,
   "",
   IF(fenlei == '总体-在职',
      "AND ISLIZHI='在职'",
      IF(fenlei == '总体-离职',
         "AND ISLIZHI='离职'",
         IF(fenlei == '主营-总体',
            "AND ISZHUYING = '主营'",
            IF(fenlei == '主营-在职',
               "AND ISZHUYING = '主营' AND AND ISLIZHI='在职'",
               IF(fenlei == '主营-离职',
                  "AND ISZHUYING = '主营' AND ISLIZHI='离职'",
                  IF(fenlei == '非主营-总体',
                     "AND ISZHUYING <>'主营'",
                     IF(fenlei == '非主营-在职',
                        "AND ISZHUYING <>'主营' AND ISLIZHI='在职'",
                        "AND ISZHUYING <>'主营' AND ISLIZHI='离职'"
                      )
                    )
                ) 
            )
          )
        )
    )
  )}
)
GROUP BY /*ISZHUYING,*/DANWEI
ORDER BY RENSHU DESC
]]></Query>
</TableData>
