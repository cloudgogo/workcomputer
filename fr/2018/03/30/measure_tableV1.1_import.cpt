<TableData name="MR_权益风险-上市普通股票明细（套保）" class="com.fr.data.impl.DBTableData">
<Parameters>
<Parameter>
<Attributes name="p_period"/>
<O>
<![CDATA[2018-02]]></O>
</Parameter>
</Parameters>
<Attributes maxMemRowCount="-1"/>
<Connection class="com.fr.data.impl.NameDatabaseConnection">
<DatabaseName>
<![CDATA[HRS]]></DatabaseName>
</Connection>
<Query>
<![CDATA[SELECT tt.*, tt.book_value * tt.rf_0 * (1 + tt.k1 + tt.k2) min_cost
  FROM (SELECT t.*,
               -- =IF(zdt>=1,1,IF(zdt>=0,zdt^2,-zdt^2)) 
               CASE
                 WHEN t.zdt > 1 THEN
                  1
                 WHEN t.zdt >= 0 THEN
                  t.zdt * t.zdt
                 ELSE
                  - (t.zdt * t.zdt)
               END k1,
               CASE
                 WHEN t.is_fengu = 'Y' THEN
                  -0.05
                 ELSE
                  0
               END k2
          FROM (SELECT c.attribute7,
                       c.segment8,
                       c.description,
                       c.modiduration,
                       c.gplx,
                       v.description description1,
                       c.is_fengu,
                       c.zhxh,
                       SUM(c.fair_value) book_value,
                       SUM(c.book_cost) cccb,
                       SUM(c.kthyj) kthyj,
                       c.tqyxx tqyxx,
                       to_char(MAX(c.last_update_date), 'yyyy-mm-dd') last_update_date,
                       --=IF(E6="沪深主板股/香港主板股",31%,IF(E6="中小板股",41%,IF(E6="创业板股/香港创业板股",48%,"")))
                       CASE
                         WHEN c.gplx = '01' THEN
                          0.31
                         WHEN c.gplx = '03' THEN
                          0.41
                         WHEN c.gplx = '02' THEN
                          0.48
                         ELSE
                          NULL
                       END rf_0,
                       CASE
                         WHEN SUM(c.book_cost) = 0 OR SUM(c.book_cost) = NULL THEN
                          0
                         ELSE
                          (SUM(c.fair_value) - SUM(c.book_cost)) /
                          SUM(c.book_cost)
                       END zdt
                  FROM hrs_core_economic_capital c,
                       (SELECT v.flex_value, v.description
                          FROM hrs_def_flex_value_sets s,
                               hrs_def_flex_values     v
                         WHERE s.flex_value_set_id = v.flex_value_set_id
                           AND s.flex_value_set_name = 'JJZB-衍生品投资-股票类型') v
                 WHERE 1=1
                  
                   AND c.period_name = '${p_period}'
              
                   AND c.attribute5 = '02'
                   AND c.gplx = v.flex_value(+)
                 GROUP BY c.attribute7,
                          c.segment8,
                          c.description,
                          c.gplx,
                          v.description,
                          c.modiduration,
                          c.is_fengu,
                          c.tqyxx,
                          c.zhxh) t) tt
 ORDER BY tt.attribute7, TRIM(tt.zhxh), tt.segment8
]]></Query>
</TableData>
<TableData name="操作风险汇总表" class="com.fr.data.impl.DBTableData">
