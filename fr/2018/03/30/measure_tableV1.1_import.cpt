         when t.period_name = substr('${p_period}', 1, 4) - 3 || '-' || '13' then
          '03'
       end attribute5,
       t.book_value
  from hrs_core_economic_capital t
 where 1 = 1
   and t.period_name in (substr('${p_period}', 1, 4) - 1 || '-' || '13',
        substr('${p_period}', 1, 4) - 2 || '-' || '13',
        substr('${p_period}', 1, 4) - 3 || '-' || '13')
   and t.segment1='104'
   and t.attribute9 = 'HFM'
   and t.item_code = 'HP100-10190'
