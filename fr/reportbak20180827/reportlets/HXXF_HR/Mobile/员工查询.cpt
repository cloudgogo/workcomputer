<JavaScript class="com.fr.js.JavaScriptImpl">
<Parameters/>
<Content>
<![CDATA[var a = this.options.form.getWidgetByName("gongzjl").getValue();
var b = this.options.form.getWidgetByName("name").getValue();
var c = this.options.form.getWidgetByName("zhuyfzy").getValue();
var d = this.options.form.getWidgetByName("yuangfl").getValue();
var e = this.options.form.getWidgetByName("zhij").getValue();
var f = this.options.form.getWidgetByName("zhinxl").getValue();
var g = this.options.form.getWidgetByName("gangw").getValue();
var h = this.options.form.getWidgetByName("CurrentTime").getValue();
var i = this.options.form.getWidgetByName("jiaoyjl").getValue();
var j = this.options.form.getWidgetByName("xuel").getValue();
var k = this.options.form.getWidgetByName("textEditor0_c").getValue();

if(a.length == 0){a1=""}
if(a.length != 0 && h =='1'){a1="AND T.WBLASTSEACOL LIKE '%"+a+"%'"}
if(a.length != 0 && h =='2'){a1="AND T.WBSEACOL LIKE '%"+a+"%'"}
if(b.length == 0){a3=""}else{a3="and T.namepinyin like'%"+b+"%'"}
/*switch(c){case "": a4="";break;case "1": a4="and T.ISzhuying like '主营%'";break;case "2" :a4="and T.ISzhuying like '非主营%'";break;case "1,2": a4="";break;}*/
if(c.length == 0){a4=""}
if(c == "1"){a4="and T.ISzhuying like '主营%'"}
if(c == "2"){a4="and T.ISzhuying like '非主营%'"}
if(c == "1','2"){a4=""}
if(d.length == 0){a5=""}else{a5="and T.YGfenlei in('"+d+"')"}
if(e.length == 0){a6=""}else{a6="and T.zhiji in('"+e+"')"}
if(f.length == 0){a7=""}else{a7="and T.zhinengxulie in('"+f+"')"}
if(g.length == 0){a8=""}else{a8="and T.gangwei like'%"+g+"%'"}
if(i.length == 0){a10=""}else{a10="AND T.JYSEACOL like'%"+i+"%'"}
if(j.length == 0){a11=""}else{a11="and T.XUELIFENLEI in('"+j+"')"}
if(k.length == 0 || k =='HX_HEAD'){a12=""}else{a12="and T.BUMENID LIKE '%"+k+"%'"}

var sql="SELECT YGID,XINGMING FROM (SELECT DISTINCT T.YGID,T.zhiji,T.xingming,T.gangwei,T.bumen,DW.TREE_NODE_NUM FROM ODM_HR_YGJLCX_SEARCH T LEFT JOIN ODM_HR_DW DW ON DW.TREE_NODE= T.BUMENID WHERE 1=1 "+a1+a3+a4+a5+a6+a7+a8+a10+a11+a12+")"
alert(sql)

var sl1 = FR.remoteEvaluate('sql("HXXF_HR","'+sql+'",1)');
alert(sl1)
var sl2 = FR.remoteEvaluate('sql("HXXF_HR","'+sql+'",2)');
alert(sl2)
var url = FR.cjkEncode("http://10.20.60.17:8080/WebReport/ReportServer?op=_custom_export_&cmd=word&_cpt_=WordDownload.cpt&_filename_="+sl2+"&id="+sl1+"&_type_=zip")
//alert(url);
window.open(url);]]></Content>
</JavaScript>
