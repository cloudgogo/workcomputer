</Parameters>
<Attributes maxMemRowCount="-1"/>
<Connection class="com.fr.data.impl.NameDatabaseConnection">
<DatabaseName>
<![CDATA[HRS]]></DatabaseName>
</Connection>
<Query>
<![CDATA[select c.attribute7,
       c.zhxh,
       c.segment8,
       c.description,
       c.MODIDURATION,
       case when c.MODIDURATION<=5 then 
       c.MODIDURATION*(-0.0019*c.MODIDURATION+0.0214)
       when c.MODIDURATION<=10 then
       c.MODIDURATION*(-0.0007*c.MODIDURATION+0.0154)
       else
       c.MODIDURATION*0.0084
       end RF_0,
       case when c.zhxh='01' then null
       else
       (case when c.MODIDURATION<=5 then 
       c.MODIDURATION*(-0.0019*c.MODIDURATION+0.0214)
       when c.MODIDURATION<=10 then
       c.MODIDURATION*(-0.0007*c.MODIDURATION+0.0154)
       else
       c.MODIDURATION*0.0084
       end)*sum(c.book_value)
       end min_book,
       
       sum(c.book_value) book_value,
       sum(c.KTHYJ) kthyj,
       c.tqyxx tqyxx,
       to_char(max(c.last_update_date), 'yyyy-mm-dd') last_update_date
  from hrs_core_economic_capital c
  where 1=1
   and c.period_name = '${p_period}'
  and c.attribute5='01'
  group by c.attribute7,
       c.zhxh,
       c.segment8,
       c.description,
       c.MODIDURATION,
       c.tqyxx
 
  

 ]]></Query>
