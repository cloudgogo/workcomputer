<TableData name="CR_可穿透信托及类信托计划明细" class="com.fr.data.impl.DBTableData">
<Parameters>
<Parameter>
<Attributes name="p_period"/>
<O>
<![CDATA[2018-02]]></O>
</Parameter>
</Parameters>
<Attributes maxMemRowCount="-1"/>
<Connection class="com.fr.data.impl.NameDatabaseConnection">
<DatabaseName>
<![CDATA[HRS]]></DatabaseName>
</Connection>
<Query>
<![CDATA[SELECT tt.*,
     tt.RF_0*(1+nvl(k1,0)+nvl(k2,0)) RF,
     round(tt.sjxyfx_book_value*(tt.RF_0*(1+nvl(k1,0)+nvl(k2,0)))/v.sjxyfx_book_value,4) RF_1
FROM (
select t.attribute4,
      t.segment8,
       t.description,
       t.zcfl,
       t.description1,
       t.cyxs,
       t.description2,
       t.jjfs,
       t.description3,
       t.attribute7,
       t.sjxyfx_book_value,
       t.purchase_value,
       t.scwz,
       t.description4,
       t.bdfd,
       t.RF_0,
       case
         when t.jjfs = '02' then
          case
         when t.bdfd >= 1 then
          1
         when t.bdfd > 0 then
          t.bdfd * t.bdfd
         else
          -1 * (t.bdfd * t.bdfd)
       end else null end k1,
       case when t.jjfs = '02' then
       case
         when t.description4 = '一线城市' then
         0.03
         when t.description4 = '二线城市' then
         0.05
         when t.description4 = '三线城市' then
        0.06
         when  t.description4 = '四线城市' then
         0.1
         when  t.description4 ='境外地区' then 
         0.06
         else 0
       end
       else null end k2
  from (select c.attribute6 attribute4,
               c.segment8,
               c.description,
               c.zcfl,
               v1.description description1,
               c.cyxs,
               v2.description description2,
               c.jjfs,
               v3.description description3,
               c.attribute7,
               c.sjxyfx_book_value,
               c.purchase_value,
               c.scwz,
               v4.description description4,
               case
                 when c.jjfs = '02' then
                  case
                 when c.purchase_value is null or c.purchase_value = 0 then
                  0
                 else
                  (c.SJXYFX_BOOK_VALUE - c.purchase_value) / c.purchase_value
               end else 0 end bdfd,
               case
                 when c.jjfs = '01' then
                  0.08
                 when c.jjfs = '02' then
                  0.12
               end RF_0,
               to_char(max(c.last_update_date),'yyyy-mm-dd') last_update_date
        
          from hrs_core_economic_capital c,
               (SELECT V.FLEX_VALUE, V.DESCRIPTION
                  FROM HRS_DEF_FLEX_VALUE_SETS S, HRS_DEF_FLEX_VALUES V
                 WHERE S.FLEX_VALUE_SET_ID = V.FLEX_VALUE_SET_ID
                   AND S.FLEX_VALUE_SET_NAME = 'JJZB-信托计划-资产分类') v1,
               (SELECT V.FLEX_VALUE, V.DESCRIPTION
                  FROM HRS_DEF_FLEX_VALUE_SETS S, HRS_DEF_FLEX_VALUES V
                
                 WHERE S.FLEX_VALUE_SET_ID = V.FLEX_VALUE_SET_ID
                   AND S.FLEX_VALUE_SET_NAME = 'JJZB-信托计划-持有形式'
                
                ) v2,
               (SELECT V.FLEX_VALUE, V.DESCRIPTION
                  FROM HRS_DEF_FLEX_VALUE_SETS S, HRS_DEF_FLEX_VALUES V
                
                 WHERE S.FLEX_VALUE_SET_ID = V.FLEX_VALUE_SET_ID
                   AND S.FLEX_VALUE_SET_NAME = 'JJZB-信托计划-计价方式'
                
                ) v3,
               (select v.flex_value, v.description
                  from hrs_def_flex_value_sets s, hrs_def_flex_values v
                 where s.flex_value_set_id = v.flex_value_set_id
                   and s.flex_value_set_name = 'JJZB-房地产-所处位置') V4,
               hrs_core_financial_product p
 where c.attribute5=p.detail_category
   and c.period_name = '${p_period}'
   and p.asset_category='160'
   and c.attribute6='Y'
           and c.zcfl = v1.flex_value(+)
           and c.cyxs = v2.flex_value(+)
           and c.jjfs = v3.flex_value(+)
           and c.scwz = v4.flex_value(+)
            group by c.attribute6,c.segment8,
               c.description,
               c.zcfl,
               v1.description ,
               c.cyxs,
               v2.description ,
               c.jjfs,
               v3.description ,
               c.attribute7,
               c.SJXYFX_BOOK_VALUE,
               c.purchase_value,
               c.scwz,
               v4.description
         order by c.zcfl, c.cyxs, c.jjfs, c.attribute7, c.segment8) t
) tt,
(select c.segment8,
               c.description,
               SUM(c.SJXYFX_BOOK_VALUE) sjxyfx_book_value
        
          from hrs_core_economic_capital c,
               hrs_core_financial_product p
 where c.attribute5=p.detail_category
   and c.period_name = '${p_period}'
   and p.asset_category='160'
   and c.attribute6='Y'
group by c.segment8,
               c.description) v
               where tt.segment8=v.segment8]]></Query>
</TableData>
<TableData name="CR_对公债权明细" class="com.fr.data.impl.DBTableData">
<Parameters>
<Parameter>
<Attributes name="p_period"/>
<O>
<![CDATA[2018-02]]></O>
</Parameter>
</Parameters>
<Attributes maxMemRowCount="-1"/>
<Connection class="com.fr.data.impl.NameDatabaseConnection">
<DatabaseName>
<![CDATA[HRS]]></DatabaseName>
</Connection>
<Query>
<![CDATA[SELECT tt.*,
       case
         when rf_0 * (1 + k1) > 1 then
          fxbl * 1
         else
          fxbl * rf_0 * (1 + k1)
       end ec

  from (SELECT t.zqzt,
               t.zqzt_name,
               t.tzxs,
               t.desc2,
               t.fxztxypj,
               t.desc1,
               t.segment8,
               t.description,
               t.book_value,
               t.xjjgzl,
               t.yszkl,
               t.fdcjtdl,
               t.ssgp,
               t.qtgql,
               t.yqrq,
               max(t.last_update_date) last_update_date,
               case
                 when (t.book_value - t.xjjgzl * 1 - t.yszkl * 0.8 -
                      t.fdcjtdl * 0.5 - t.ssgp * 0.45 - t.qtgql * 0.25) >
                      t.book_value * 0.1 then
                  (t.book_value - t.xjjgzl * 1 - t.yszkl * 0.8 -
                  t.fdcjtdl * 0.5 - t.ssgp * 0.45 - t.qtgql * 0.25)
                 else
                  t.book_value * 0.1
               end fxbl,
               t.yqsj,
               t.rf_0,
               CASE
                 WHEN t.yqsj = 0 THEN
                  0
                 WHEN t.yqsj <= 90 THEN
                  0.2
                 WHEN t.yqsj < 180 THEN
                  1.4
                 WHEN t.yqsj < 1095 THEN
                  3.8
                 ELSE
                  7
               END k1
          FROM (SELECT c.attribute5 zqzt,
                       c.prod_type2 zqzt_name,
                       '' tzxs,
                       '' desc2,
                       CASE
                         WHEN c.convertable IN ('AAA', 'A-1') THEN
                          'AAA'
                         WHEN c.convertable = 'AA+' THEN
                          'AA+'
                         WHEN c.convertable = 'AA' THEN
                          'AA'
                         WHEN c.convertable = 'AA-' THEN
                          'AA'
                         WHEN c.convertable IN ('A+', 'A', 'A-') THEN
                          'A+、A、A-'
                         ELSE
                          'BBB+、BBB、BBB-、无评级'
                       end fxztxypj,
                       v.description desc1,
                       c.segment8,
                       c.description,
                       c.book_value,
                       c.xjjgzl,
                       c.yszkl,
                       c.fdcjtdl,
                       c.ssgp,
                       c.qtgql,
                       c.yqrq,
                       to_char(MAX(c.last_update_date), 'yyyy-mm-dd') last_update_date,
                       CASE
                         WHEN to_date(c.yqrq, 'yyyy-mm-dd') IS NULL THEN
                          0
                         ELSE
                          round(MAX(c.last_update_date) -
                                to_date(c.yqrq, 'yyyy-mm-dd'))
                       END yqsj,
                       CASE
                         WHEN c.convertable IN ('AAA', 'A-1') THEN
                          0.01
                         WHEN c.convertable = 'AA+' THEN
                          0.031
                         WHEN c.convertable = 'AA' THEN
                          0.04
                         WHEN c.convertable = 'AA-' THEN
                          0.044
                         WHEN c.convertable IN ('A+', 'A', 'A-') THEN
                          0.085
                         ELSE
                          0.125
                       end rf_0
                  FROM hrs_core_economic_capital c,
                       (SELECT v.flex_value, v.description
                          FROM hrs_def_flex_value_sets s,
                               hrs_def_flex_values     v
                         WHERE s.flex_value_set_id = v.flex_value_set_id
                           AND s.flex_value_set_name = 'JJZB-综合类-外部债项评级') v,
                       hrs_core_financial_product p
                 where c.attribute5 = p.detail_category
                   AND c.period_name = '${p_period}'
                   AND c.attribute5 = '8450'
                   and p.asset_category = '180'
                   AND c.fxztxypj = v.flex_value(+)
                 GROUP BY c.attribute5,
                          c.prod_type2,
                          CASE
                            WHEN c.convertable IN ('AAA', 'A-1') THEN
                             'AAA'
                            WHEN c.convertable = 'AA+' THEN
                             'AA+'
                            WHEN c.convertable = 'AA' THEN
                             'AA'
                            WHEN c.convertable = 'AA-' THEN
                             'AA'
                            WHEN c.convertable IN ('A+', 'A', 'A-') THEN
                             'A+、A、A-'
                            ELSE
                             'BBB+、BBB、BBB-、无评级'
                          end,
                          c.convertable,
                          v.description,
                          c.segment8,
                          c.description,
                          c.book_value,
                          c.xjjgzl,
                          c.yszkl,
                          c.fdcjtdl,
                          c.ssgp,
                          c.qtgql,
                          c.yqrq
                UNION ALL
                
                SELECT c.attribute5 zqzt,
                       c.prod_type2 zqzt_name,
                       c.tzxs,
                       v2.description desc2,
                       c.cjfl,
                       v3.description desc1,
                       c.segment8,
                       c.description,
                       c.book_value,
                       c.xjjgzl,
                       c.yszkl,
                       c.fdcjtdl,
                       c.ssgp,
                       c.qtgql,
                       c.yqrq,
                       to_char(MAX(c.last_update_date), 'yyyy-mm-dd') last_update_date,
                       CASE
                         WHEN to_date(c.yqrq, 'yyyy-mm-dd') IS NULL THEN
                          0
                         ELSE
                          round(MAX(c.last_update_date) -
                                to_date(c.yqrq, 'yyyy-mm-dd'))
                       END yqsj,
                       CASE
                         WHEN c.tzxs = '210' THEN
                          (CASE
                         WHEN c.cjfl = '2110' THEN
                          0.125
                         WHEN c.cjfl = '2120' THEN
                          0.25
                       END) WHEN c.tzxs = '220' THEN 0.125 END rf_0
                  FROM hrs_core_economic_capital c,
                       /**(SELECT v.flex_value,
                                              v.description,
                                              v.flex_value || ' ' || v.description
                                         FROM hrs_def_flex_value_sets s, hrs_def_flex_values v
                                        WHERE s.flex_value_set_id = v.flex_value_set_id
                                          AND s.flex_value_set_name = 'JJZB-BC-债权主体') v1,**/
                       (SELECT v.flex_value,
                               v.description,
                               v.flex_value || ' ' || v.description
                          FROM hrs_def_flex_value_sets s,
                               hrs_def_flex_values     v
                         WHERE s.flex_value_set_id = v.flex_value_set_id
                           AND s.flex_value_set_name = 'JJZB_BC_INVEST_TYPE') v2,
                       (SELECT v.flex_value,
                               v.description,
                               v.flex_value || ' ' || v.description
                          FROM hrs_def_flex_value_sets s,
                               hrs_def_flex_values     v
                         WHERE s.flex_value_set_id = v.flex_value_set_id
                           AND s.flex_value_set_name = 'JJZB_BC_PROD_SUB_CATE') v3,
                       hrs_core_financial_product p
                 where c.attribute5 = p.detail_category
                      
                   AND c.period_name = '${p_period}'
                   and p.asset_category = '120'
                      --AND c.zqzt = v1.flex_value(+)
                   and c.attribute5 = '2940'
                   AND c.tzxs = v2.flex_value(+)
                   AND c.cjfl = v3.flex_value(+)
                 GROUP BY c.attribute5,
                          c.prod_type2,
                          c.cjfl,
                          c.tzxs,
                          
                          v2.description,
                          v3.description,
                          c.segment8,
                          c.description,
                          c.book_value,
                          c.xjjgzl,
                          c.yszkl,
                          c.fdcjtdl,
                          c.ssgp,
                          c.qtgql,
                          c.yqrq) t
         group by t.zqzt,
                  t.zqzt_name,
                  t.tzxs,
                  t.desc2,
                  t.fxztxypj,
                  t.desc1,
                  t.segment8,
                  t.description,
                  t.book_value,
                  t.xjjgzl,
                  t.yszkl,
                  t.fdcjtdl,
                  t.ssgp,
                  t.qtgql,
                  t.yqrq,
                  t.yqsj,
                  t.rf_0) tt
]]></Query>
</TableData>
