<TableData name="MR_汇率风险（未套保权益类资产）" class="com.fr.data.impl.DBTableData">
<Parameters>
<Parameter>
<Attributes name="p_period"/>
<O>
<![CDATA[2018-02]]></O>
</Parameter>
</Parameters>
<Attributes maxMemRowCount="-1"/>
<Connection class="com.fr.data.impl.NameDatabaseConnection">
<DatabaseName>
<![CDATA[HRS]]></DatabaseName>
</Connection>
<Query>
<![CDATA[select * from (
select c.attribute4,
       c.currency_code,
       c.attribute5,
       c.prod_type2,
       sum(c.book_value) book_value,
       to_char(max(c.last_update_date),'yyyy-mm-dd') last_update_date,
       '美元或与美元挂钩' type
  from hrs_core_economic_capital c,
    HRS_CORE_FINANCIAL_PRODUCT p
 where c.attribute5=p.detail_category
   and c.period_name = '${p_period}'
   and c.currency_code='USD'
   and p.asset_category='210'
   and c.attribute4 is not null
   and c.tbqk='01'
 group by c.attribute4,
        c.currency_code,
       c.attribute5,
       c.prod_type2

  union all

  select c.attribute4,
       c.currency_code,
       c.attribute5,
       c.prod_type2,
       sum(c.book_value) book_value,
       to_char(max(c.last_update_date),'yyyy-mm-dd') last_update_date,
       '其他币种' type
  from hrs_core_economic_capital c,
    HRS_CORE_FINANCIAL_PRODUCT p
 where c.attribute5=p.detail_category
   and c.period_name = '${p_period}'
   and p.asset_category='210'
   and c.currency_code<>'USD'
   and c.attribute4 is not null
   and c.tbqk='01'
 group by c.attribute4,
        c.currency_code,
       c.attribute5,
       c.prod_type2
       
       ) t
  order by t.attribute4 desc,t.type desc

  ]]></Query>
</TableData>
<TableData name="MR_汇率风险（净额绝对值）" class="com.fr.data.impl.DBTableData">
<Parameters>
<Parameter>
<Attributes name="p_period"/>
<O>
<![CDATA[2018-02]]></O>
</Parameter>
</Parameters>
<Attributes maxMemRowCount="-1"/>
<Connection class="com.fr.data.impl.NameDatabaseConnection">
<DatabaseName>
<![CDATA[HRS]]></DatabaseName>
</Connection>
<Query>
<![CDATA[SELECT t.*,
       CASE
         WHEN t.tbqk = '01' THEN
          (CASE
            WHEN t.type = '美元或与美元挂钩' THEN
             t.book_value * 0.035 * (1 + 0.00)
            ELSE
             t.book_value * 0.035 * (1 + 0.12)
          END)
         WHEN t.tbqk = '02' THEN
          (CASE
            WHEN t.type = '美元或与美元挂钩' THEN
             abs(t.book_value - t.mybj*(nvl(t.tqyxx,0))) * 0.035 * (1 + 0.00)
            ELSE
             abs(t.book_value - t.mybj*(nvl(t.tqyxx,0))) * 0.035 * (1 + 0.12)
          END)
         ELSE
          (CASE
            WHEN t.type = '美元或与美元挂钩' THEN
             t.mybj * 0.035 * (1 + 0.00)
            ELSE
             t.mybj * 0.035 * (1 + 0.12)
          END)
       END book_value2
  FROM (SELECT c.attribute4,
               c.tbqk,
               c.currency_code,
               c.tqyxx,
               SUM(CASE
                     WHEN c.attribute5 = '9510' THEN
                      (-1) * c.book_value
                     ELSE
                      c.book_value
                   END) book_value,
               SUM(c.mybj) mybj,
               to_char(MAX(c.last_update_date), 'yyyy-mm-dd') last_update_date,
               '美元或与美元挂钩' TYPE
          FROM hrs_core_economic_capital c, hrs_core_financial_product p
         WHERE c.attribute5 = p.detail_category
         
           AND c.period_name = '${p_period}'
           AND c.currency_code = 'USD'
           AND p.asset_category = '210'
          -- AND c.attribute4 IS NOT NULL
           AND (c.attribute5 NOT IN ('9500', '9520') OR c.attribute5 IS NULL)
         GROUP BY c.tbqk, c.attribute4, c.currency_code,
               c.tqyxx
        
        UNION ALL
        
        SELECT c.attribute4,
               c.tbqk,
               c.currency_code,
               c.tqyxx,
               SUM(CASE
                     WHEN c.attribute5 = '9510' THEN
                      (-1) * c.book_value
                     ELSE
                      c.book_value
                   END) book_value,
               SUM(c.mybj) mybj,
               to_char(MAX(c.last_update_date), 'yyyy-mm-dd') last_update_date,
               '其他币种' TYPE
          FROM hrs_core_economic_capital c, hrs_core_financial_product p
         WHERE c.attribute5 = p.detail_category
           
           AND c.period_name = '${p_period}'
           AND c.currency_code <> 'USD'
           AND p.asset_category = '210'
           --AND c.attribute4 IS NOT NULL
           AND (c.attribute5 NOT IN ('9500', '9520') OR c.attribute5 IS NULL)
         GROUP BY c.currency_code, c.attribute4, c.tbqk,
               c.tqyxx
        
        ) t
 ORDER BY t.attribute4 DESC
]]></Query>
</TableData>
<TableData name="MR_利率风险_债券类资产(未套保)" class="com.fr.data.impl.DBTableData">
