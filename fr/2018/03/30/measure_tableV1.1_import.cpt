<TableData name="MR_权益风险-上市普通股票明细（未套保）" class="com.fr.data.impl.DBTableData">
<Parameters>
<Parameter>
<Attributes name="p_period"/>
<O>
<![CDATA[2018-02]]></O>
</Parameter>
</Parameters>
<Attributes maxMemRowCount="-1"/>
<Connection class="com.fr.data.impl.NameDatabaseConnection">
<DatabaseName>
<![CDATA[HRS]]></DatabaseName>
</Connection>
<Query>
<![CDATA[select z.*,
       case when zdf>=1 then 1
       when zdf<1 then zdf*zdf
       else -(zdf*zdf) end k1,
       case when z.fair_value*z.RF_0*(1+(case when zdf>=1 then 1
       when zdf<1 then zdf*zdf
       else -(zdf*zdf) end)+Z.K2) is null
       then 0
       else
       z.fair_value*z.RF_0*(1+(case when zdf>=1 then 1
       when zdf<1 then zdf*zdf
       else -(zdf*zdf) end)+Z.K2)
       end EC

from (
select t.attribute4,
       t.description,
       t.segment8,
       t.cjfl,
       t.is_fengu,
       t.book_cost,
       t.book_value fair_value,
       v1.description desc1,
       case when t.book_cost is null or t.book_cost=0
       then 0
       else
       (t.book_value-t.book_cost)/t.book_cost
       end zdf,
       case when v1.description='沪深板股/香港主板股'
       then 0.31
       when v1.description='中小板股'
       then 0.41
       WHEN v1.description='创业板股/香港创业板股'
       THEN 0.48
       else null
       end RF_0,
       case when t.is_fengu='Y' THEN -0.05
       ELSE 0
       END K2,
       to_char(max(t.last_update_date), 'yyyy-mm-dd') last_update_date1
  from hrs_core_economic_capital t,
       (select v.flex_value, v.description
          from hrs_def_flex_value_sets s, hrs_def_flex_values v
         where s.flex_value_set_id = v.flex_value_set_id
           and s.flex_value_set_name = 'JJZB_BC_PROD_SUB_CATE') v1
 where 1 = 1
   
   and t.period_name = '${p_period}'
   and t.attribute5 = '1010'
   and t.cjfl = v1.flex_value(+)
group by t.attribute4,
       t.description,
       t.segment8,
       t.cjfl,
       t.is_fengu,
       t.book_cost,
       t.book_value,
       v1.description
 
)z
order by z.attribute4 desc,z.segment8,z.cjfl]]></Query>
</TableData>
<TableData name="CR_信托及类信托计划汇总表" class="com.fr.data.impl.DBTableData">
<Parameters>
<Parameter>
<Attributes name="p_period"/>
<O>
<![CDATA[2018-02]]></O>
</Parameter>
</Parameters>
<Attributes maxMemRowCount="-1"/>
<Connection class="com.fr.data.impl.NameDatabaseConnection">
<DatabaseName>
<![CDATA[HRS]]></DatabaseName>
</Connection>
<Query>
<![CDATA[SELECT m.* ,
       m.book_value*m.RF_0*(1+nvl(m.k1,0)+nvl(m.k2,0)) EC
FROM (
select t1.*,
case when t1.attribute4='Y' then
t2.RF_1
else
0.3
end RF_0
from(
select c.attribute6 attribute4,
       c.segment8,
       c.description,
       c.convertable xypj,
       v1.description description1,
       c.convertable fxztxypj,
       v2.description description2,
       c.yxlhj,
       v3.description description3,
       sum(c.book_value) book_value,
       to_char(max(c.last_update_date), 'yyyy-mm-dd') last_update_date,
       case when v2.description='AAA' THEN 0
        when v2.description='AA+' THEN 0.1
        when v2.description='AA' THEN 0.2
        when v2.description='AA-' THEN 0.3
        when v2.description in ('A+','A','A-') THEN 0.4
        when v2.description='BBB+、BBB、BBB-、无评级' then
         0.5
         else null
        end k1,
        --=IF(G8="优先级",-0.2,IF(G8="优先级",0.2,0))
        case when v3.description='优先级' then -0.2
        when v3.description='劣后级' then 0.2
        else
        0
        end k2
  from hrs_core_economic_capital c,
       (SELECT V.FLEX_VALUE, V.DESCRIPTION
          FROM HRS_DEF_FLEX_VALUE_SETS S, HRS_DEF_FLEX_VALUES V
         WHERE S.FLEX_VALUE_SET_ID = V.FLEX_VALUE_SET_ID
           AND S.FLEX_VALUE_SET_NAME = 'JJZB-BC-信用评级') v1,
       (SELECT V.FLEX_VALUE, V.DESCRIPTION
          FROM HRS_DEF_FLEX_VALUE_SETS S, HRS_DEF_FLEX_VALUES V
        
         WHERE S.FLEX_VALUE_SET_ID = V.FLEX_VALUE_SET_ID
           AND S.FLEX_VALUE_SET_NAME = 'JJZB-BC-信用评级'
        
        ) v2,
       (SELECT V.FLEX_VALUE, V.DESCRIPTION
          FROM HRS_DEF_FLEX_VALUE_SETS S, HRS_DEF_FLEX_VALUES V
        
         WHERE S.FLEX_VALUE_SET_ID = V.FLEX_VALUE_SET_ID
           AND S.FLEX_VALUE_SET_NAME = 'JJZB-BC-分级类型'
        
        ) v3,
   hrs_core_financial_product p
 where c.attribute5=p.detail_category
  

   and c.period_name = '${p_period}'
  
   and p.asset_category='160'
   and c.convertable= v1.flex_value(+)
   and c.convertable = v2.flex_value(+)
   and c.yxlhj = v3.flex_value(+)
 group by c.attribute6,
       c.segment8,
       c.description,
       c.convertable,
       v1.description,
       v2.description,
       c.yxlhj,
       v3.description
 order by c.attribute6 desc,c.segment8) t1,
 (SELECT tt.*,
     tt.RF_0*(1+nvl(k1,0)+nvl(k2,0)) RF,
     round(tt.book_value*(tt.RF_0*(1+nvl(k1,0)+nvl(k2,0)))/v.sjxyfx_book_value,4) RF_1
FROM (
select t.attribute4,t.segment8,
       t.description,
       t.zcfl,
       t.description1,
       t.cyxs,
       t.description2,
       t.jjfs,
       t.description3,
       t.attribute7,
       t.book_value,
       t.purchase_value,
       t.scwz,
       t.description4,
       t.bdfd,
       t.RF_0,
       case
         when t.jjfs = '02' then
          case
         when t.bdfd >= 1 then
          1
         when t.bdfd > 0 then
          t.bdfd * t.bdfd
         else
          -1 * (t.bdfd * t.bdfd)
       end else null end k1,
       case when t.jjfs = '02' then
       case
         when t.description4 = '一线城市' then
         0.03
         when t.description4 = '二线城市' then
         0.05
         when t.description4 = '三线城市' then
        0.06
         when  t.description4 = '四线城市' then
         0.1
         when  t.description4 ='境外地区' then 
         0.06
         else 0
       end
       else null end k2
  from (select c.attribute6 attribute4,c.segment8,
               c.description,
               c.zcfl,
               v1.description description1,
               c.cyxs,
               v2.description description2,
               c.jjfs,
               v3.description description3,
               c.attribute7,
               c.book_value,
               c.purchase_value,
               c.scwz,
               v4.description description4,
               case
                 when c.jjfs = '02' then
                  case
                 when c.purchase_value is null or c.purchase_value = 0 then
                  0
                 else
                  (c.book_value - c.purchase_value) / c.purchase_value
               end else 0 end bdfd,
               case
                 when c.jjfs = '01' then
                  0.08
                 when c.jjfs = '02' then
                  0.12
               end RF_0,
               to_char(max(c.last_update_date),'yyyy-mm-dd') last_update_date
        
          from hrs_core_economic_capital c,
               (SELECT V.FLEX_VALUE, V.DESCRIPTION
                  FROM HRS_DEF_FLEX_VALUE_SETS S, HRS_DEF_FLEX_VALUES V
                 WHERE S.FLEX_VALUE_SET_ID = V.FLEX_VALUE_SET_ID
                   AND S.FLEX_VALUE_SET_NAME = 'JJZB-信托计划-资产分类') v1,
               (SELECT V.FLEX_VALUE, V.DESCRIPTION
                  FROM HRS_DEF_FLEX_VALUE_SETS S, HRS_DEF_FLEX_VALUES V
                
                 WHERE S.FLEX_VALUE_SET_ID = V.FLEX_VALUE_SET_ID
                   AND S.FLEX_VALUE_SET_NAME = 'JJZB-信托计划-持有形式'
                
                ) v2,
               (SELECT V.FLEX_VALUE, V.DESCRIPTION
                  FROM HRS_DEF_FLEX_VALUE_SETS S, HRS_DEF_FLEX_VALUES V
                
                 WHERE S.FLEX_VALUE_SET_ID = V.FLEX_VALUE_SET_ID
                   AND S.FLEX_VALUE_SET_NAME = 'JJZB-信托计划-计价方式'
                
                ) v3,
               (select v.flex_value, v.description
                  from hrs_def_flex_value_sets s, hrs_def_flex_values v
                 where s.flex_value_set_id = v.flex_value_set_id
                   and s.flex_value_set_name = 'JJZB-房地产-所处位置') V4,
   hrs_core_financial_product p
 where c.attribute5=p.detail_category

   and c.period_name = '${p_period}'

   --and c.attribute6='N'
   and p.asset_category='160'
           and c.zcfl = v1.flex_value(+)
           and c.cyxs = v2.flex_value(+)
           and c.jjfs = v3.flex_value(+)
           and c.scwz = v4.flex_value(+)
            group by c.attribute6,c.segment8,
               c.description,
               c.zcfl,
               v1.description ,
               c.cyxs,
               v2.description ,
               c.jjfs,
               v3.description ,
               c.attribute7,
               c.book_value,
               c.purchase_value,
               c.scwz,
               v4.description
         order by c.zcfl, c.cyxs, c.jjfs, c.attribute7, c.segment8) t
) tt,
(select c.segment8,
               c.description,
               SUM(c.book_value) sjxyfx_book_value
        
          from hrs_core_economic_capital c,
   hrs_core_financial_product p
 where c.attribute5=p.detail_category

   and c.period_name = '${p_period}'
   and p.asset_category='160'
  --and c.attribute6='N'
group by c.segment8,
               c.description) v
               where tt.segment8=v.segment8) t2
               where t1.segment8=t2.segment8)  m
]]></Query>
</TableData>
