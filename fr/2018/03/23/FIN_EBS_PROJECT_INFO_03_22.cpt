<![CDATA[=sql("HRS","           select distinct
                r.dept_code
           from hrs_def_relationship_set rs, hrs_def_relationship r
          where rs.relationship_set_name =
                'HRS_LEDGER_COSTCENTER_RELATIONSHIP'
                and r.ledger_id =1001
                and  r.dept_code is not null
                  AND EXISTS (SELECT 1
          FROM  hrs_core_dim_value_hierarchy h
         WHERE 1 = 1
           AND h.dim_value = 'C131107'
           AND  r.dept_code BETWEEN h.child_dim_value_low AND
       h.child_dim_value_high)
            order by r.dept_code
                ",1)]]></Attributes>
</O>
