<Connection class="com.fr.data.impl.NameDatabaseConnection">
<DatabaseName>
<![CDATA[HXXF_HR]]></DatabaseName>
</Connection>
<Query>
<![CDATA[SELECT POSITION_NBR,
       T.DESCR TDESCR,
       T2.DESCR T2DESCR,
       XLATLONGNAME GBSF,
       CASE WHEN HX_MGRDIRECT_FLAG='Y' THEN '是' ELSE '否' END ISHXGB,
       CASE WHEN HX_BANZI_FLAG='Y' THEN '是' ELSE '否' END ISBZ,
       CASE WHEN HX_MGRHEAD_FLAG='Y' THEN '是' ELSE '否' END ISYBS
  FROM ODS_HR_PS_POSITION_DATA T
  LEFT JOIN ODS_HR_PSXLATITEM T1
    ON HX_AWE_IDENTITY = T1.FIELDVALUE
  LEFT JOIN ODS_HR_PS_BUS_UNIT_TBL_HR T2
    ON T.BUSINESS_UNIT=T2.BUSINESS_UNIT
    
 WHERE XLATLONGNAME in ('${DW}')
   AND T.EFF_STATUS = 'A'
   AND HX_MGRHEAD_FLAG='Y']]></Query>
</TableData>
</TableDataMap>
