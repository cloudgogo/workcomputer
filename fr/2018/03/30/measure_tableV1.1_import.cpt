<TableData name="MR_利率风险-利率互换" class="com.fr.data.impl.DBTableData">
<Parameters>
<Parameter>
<Attributes name="p_period"/>
<O>
<![CDATA[2018-02]]></O>
</Parameter>
</Parameters>
<Attributes maxMemRowCount="-1"/>
<Connection class="com.fr.data.impl.NameDatabaseConnection">
<DatabaseName>
<![CDATA[HRS]]></DatabaseName>
</Connection>
<Query>
<![CDATA[SELECT c.attribute4,
       c.segment8,
       c.description,
       c.hylx,
       c.modiduration,
       SUM(c.book_value) book_value,
       case when c.hylx = '收固定利率 付浮动利率'then
          (case when c.modiduration <= 5
             then SUM(c.book_value)*round(ABS(c.modiduration) * (-0.0019 * ABS(c.modiduration) + 0.0214), 4)
             
             when c.modiduration <= 10
              then  SUM(c.book_value)*round(ABS(c.modiduration) * (-0.0007 * ABS(c.modiduration) + 0.0154), 4)
              else  SUM(c.book_value)*round(ABS(c.modiduration) * 0.0084, 4) end)
       else
          (case when c.modiduration <= 5
            then SUM(c.book_value)*round(-ABS(c.modiduration) * (-0.0019 * ABS(c.modiduration) + 0.0214), 4)
             when c.modiduration <= 10
               then SUM(c.book_value)*round(-ABS(c.modiduration) * (-0.0007 * ABS(c.modiduration) + 0.0154), 4)
              else  SUM(c.book_value)*round(-ABS(c.modiduration) * 0.0084, 4)end)
              end EC,
                to_char(MAX(c.last_update_date), 'yyyy-mm-dd') last_update_date
  FROM hrs_core_economic_capital c
 WHERE 1=1
         AND c.period_name = '${p_period}'
   AND c.attribute5 = '7130'
 GROUP BY c.attribute4, c.segment8, c.description, c.hylx, c.modiduration
 ORDER BY c.attribute4 DESC, c.segment8
]]></Query>
</TableData>
