<TableData name="2_2_1_入职干部上一家雇主分析_雇主类型" class="com.fr.data.impl.DBTableData">
<Parameters>
<Parameter>
<Attributes name="seadate1"/>
<O>
<![CDATA[]]></O>
</Parameter>
<Parameter>
<Attributes name="DWMC"/>
<O>
<![CDATA[]]></O>
</Parameter>
</Parameters>
<Attributes maxMemRowCount="-1"/>
<Connection class="com.fr.data.impl.NameDatabaseConnection">
<DatabaseName>
<![CDATA[HXXF_HR]]></DatabaseName>
</Connection>
<Query>
<![CDATA[WITH 
A AS(
SELECT *
  FROM ODM_HR_YGJLCX_WBGZLL_VW VW
  WHERE ENDTIME = (
                  SELECT MAX(ENDTIME)
                    FROM ODM_HR_YGJLCX_WBGZLL_VW B
                   WHERE B.YGID = VW.YGID
)
)

SELECT *
  FROM(
SELECT A.GZLEIXING,
       COUNT(YGTZ.YGID) AS RENSHU
  FROM ODM_HR_YGTZ YGTZ
  LEFT JOIN A
    ON YGTZ.YGID = A.YGID
   WHERE YGFENLEI = '干部'
      AND A.GZLEIXING IS NOT NULL
${if(len(seadate1)==0 ,
"AND TO_CHAR(RZDATE,'YYYY') = TO_CHAR(SYSDATE,'YYYY')",
"AND TO_CHAR(RZDATE,'YYYY') = '"+seadate1+"'")}
   ${if(len(DWMC)=0,"and 1=1"," and TREE_NODE_ID LIKE '%"+DWMC+",%'")}
   GROUP BY GZLEIXING
   ORDER BY RENSHU DESC
   )
   


UNION ALL

   SELECT '自主培养' AS GZLEIXING,
          COUNT(YGTZ.YGID) AS RENSHU
  FROM ODM_HR_YGTZ YGTZ
  LEFT JOIN A
    ON YGTZ.YGID = A.YGID
   WHERE  YGFENLEI = '干部'
     AND A.GZLEIXING IS NULL
     AND (YGTZ.ISCQT = '是'
         OR
        (YGTZ.CJGZDATE = YGTZ.RZDATE
        )
        )
${if(len(seadate1)==0 ,
"AND TO_CHAR(RZDATE,'YYYY') = TO_CHAR(SYSDATE,'YYYY')",
"AND TO_CHAR(RZDATE,'YYYY') = '"+seadate1+"'")}
   ${if(len(DWMC)=0,"and 1=1"," and TREE_NODE_ID LIKE '%"+DWMC+",%'")}
   GROUP BY GZLEIXING


UNION ALL

SELECT CASE WHEN ${if(len(DWMC)==0,"1=1","'"+DWMC+"'='HX_HEAD'")}  
                 OR  TREE_NODE_ID LIKE '%100000,%'
            THEN '其他（主要为国际）' 
            ELSE '其他' 
        END AS GZLEIXING,
          COUNT(YGTZ.YGID) AS RENSHU
  FROM ODM_HR_YGTZ YGTZ
  LEFT JOIN A
    ON YGTZ.YGID = A.YGID
   WHERE YGTZ.YGFENLEI = '干部'
     AND A.GZLEIXING IS NULL
     AND YGTZ.ISCQT <> '是'
     AND YGTZ.CJGZDATE <> YGTZ.RZDATE
${if(len(seadate1)==0 ,
"AND TO_CHAR(RZDATE,'YYYY') = TO_CHAR(SYSDATE,'YYYY')",
"AND TO_CHAR(RZDATE,'YYYY') = '"+seadate1+"'")}
   ${if(len(DWMC)=0,"and 1=1"," and TREE_NODE_ID LIKE '%"+DWMC+",%'")}
   GROUP BY CASE WHEN ${if(len(DWMC)==0,"1=1","'"+DWMC+"'='HX_HEAD'")}  
                 OR  TREE_NODE_ID LIKE '%100000,%'
            THEN '其他（主要为国际）' 
            ELSE '其他' 
             END]]></Query>
</TableData>
