</Parameters>
<Attributes maxMemRowCount="-1"/>
<Connection class="com.fr.data.impl.NameDatabaseConnection">
<DatabaseName>
<![CDATA[HRS]]></DatabaseName>
</Connection>
<Query>
<![CDATA[select t.attribute4,
       t.segment8,
       t.Description,
       t.attribute5,
       case when t.attribute5 in ('2010') then
       '政府债' when t.attribute5 in ('2030','2040') then
       '准政府债' when t.attribute5 in ('2110','2120','2130') then
       '金融债' when t.attribute5 in ('2060','2210','2220','2310','2320','2330','2610','2510') then
       '企业（公司）债'
       end PROD_TYPE2,
    --   t.PROD_TYPE2,
       t.modiduration,
       t.fair_value,
       t.last_update_date
from (
select c.attribute4,
       c.segment8,
       c.Description,
       c.attribute5,
       p.detail_category_name PROD_TYPE2,
       c.modiduration,
       SUM(c.book_value) fair_value,
       to_char(max(c.last_update_date), 'yyyy-mm-dd') last_update_date
  from hrs_core_economic_capital c,
   hrs_core_financial_product p
 where c.attribute5=p.detail_category
   and c.period_name = '${p_period}'
   and p.asset_category='110'
   and c.attribute5 not in ('2060','2210','2220','2310','2320','2330','2610','2510')
 group by c.attribute4,
          c.segment8,
          c.Description,
          c.attribute5,
          p.detail_category_name,
          c.modiduration
 union all

 select c.attribute4,
       c.segment8,
       c.Description,
       c.attribute5,
       p.detail_category_name PROD_TYPE2,
       c.modiduration,
       SUM(c.book_value) fair_value,
       to_char(max(c.last_update_date), 'yyyy-mm-dd') last_update_date
  from hrs_core_economic_capital c,
   hrs_core_financial_product p
 where c.attribute5=p.detail_category
   and c.period_name = '${p_period}'
   and p.asset_category='110'
   and c.attribute5 in ('2060','2210','2220','2310','2320','2330','2610','2510')
   and c.financial_type=10
 group by c.attribute4,
          c.segment8,
          c.Description,
          c.attribute5,
          p.detail_category_name,
          c.modiduration
 ) t
  order by t.attribute4 desc,t.attribute5,t.segment8

]]></Query>
